import random

uppercase_letter = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
lowercase_letter = uppercase_letter.lower()
digits= "0123456789"
symbols= " ()[],;:_\/?+#$^% "

upper,lower,nums,syns=True,True,True,True

all = ""

if upper :

          all += uppercase_letter
if lower : 

           all += lowercase_letter

if nums :
         all += digits 

if syns :
         all += symbols
        


length= input("enter the lenght:")

amount=input("enter size:")


for x in range (amount):
        password= "".join(random.sample(all,length))
        print(password)




                         




